{
    "name": "avtelma",
    "version": "1.0.0",
    "description": "Auth server use http://localhost:8000\r Front-end use http://localhost:3000",
    "main": "index.js",
    "scripts": {
        "front-end:dev": "npm run --prefix front-end dev",
        "AUTH_server:dev": "npm run --prefix AUTH_server dev",
        "start": "cross-env NODE_ENV=development concurrently --kill-others \"npm run front-end:dev\" \"npm run AUTH_server:dev\"",
        "front-end:install": "npm run --prefix front-end install",
        "AUTH_server:install": "npm run --prefix AUTH_server install",
        "install": "concurrently \"npm run front-end:install\" \"npm run AUTH_server:install\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^6.2.1",
        "cross-env": "^7.0.3",
        "nodemon": "^2.0.12"
    },
    "dependencies": {
        "react-bodymovin": "^2.0.0"
    }
}
